public class methArray
{ 
  public static void printArray(int[] a)
  {
    int[] value = a;
    for (int i=0; i < value.length; i++)
    {
      System.out.println(value[i]);
    }
  }
  
  public static void main(String[] args)
  { 
    int[] a = new int[]{1,5,10,25,50,100};
    printArray(a);
  }
}
