public class subjectMethod
{ 
  public static String makeSubjectLine(String a)
  {
     String value = "Subject: ";
     value = value + a;
     return value;
  }
  
  public static void main(String[] args)
  { 
    String subject = "What a wonderfull day";
    String value = makeSubjectLine(subject);
    System.out.println(value);
  }
}
